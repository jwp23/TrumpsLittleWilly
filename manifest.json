{
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Trumps Little Willy"
  },
  "content_scripts": [
    {
      "js": [
        "content_script.js"
      ],
      "matches": [
        "*://*/*"
      ],
      "run_at": "document_end"
    }
  ],
  "description": "This extension will replace all instances of \"Trump\" and \"Drumpf\" with \"Trump's Little Willy\" or \"Drumpf's Little Willy\"",
  "manifest_version": 2,
  "name": "Trump's Little Willy",
  "short_name": "Trumps Willy",
  "permissions": [
    "http://*.google.com/",
    "http://*.com/",
    "tabs"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0"
}
